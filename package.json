{
  "name": "tubecaster",
  "version": "0.3.2",
  "description": "Turn YouTube playlists into video podcasts",
  "homepage": "https://github.com/cprecioso/tubecaster#readme",
  "bugs": {
    "url": "https://github.com/cprecioso/tubecaster/issues"
  },
  "license": "ISC",
  "author": "Carlos Precioso <kalprestito@gmail.com>",
  "files": [
    "lib",
    "public/**/*.styl",
    "views"
  ],
  "main": "lib/index.js",
  "bin": {
    "tubecaster": "lib/start.js"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://cprecioso@github.com/cprecioso/tubecaster.git"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "format": "prettier --write \"src/**/*\" \"types/**/*\"",
    "prepublish": "npm run clean && tsc -p .",
    "start": "node lib/start.js",
    "start:nocache": "TUBECASTER_CACHE=no node lib/start.js",
    "watch": "npm run clean && tsc --watch -p ."
  },
  "dependencies": {
    "apicache": "^1.2.1",
    "axios": "^0.18.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "most": "^1.7.3",
    "node-cache": "^4.1.1",
    "parse-author": "^2.0.0",
    "pug": "^2.0.1",
    "rss": "^1.2.2",
    "stylus": "^0.54.5",
    "ytdl-core": "^0.20.2"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "@types/node": "^9.4.7",
    "@types/rss": "^0.0.28",
    "prettier": "^1.11.1",
    "typescript": "^2.7.2"
  },
  "engines": {
    "node": ">=8"
  },
  "engineStrict": true
}
