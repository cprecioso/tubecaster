{
  "name": "tubecaster",
  "version": "0.4.0",
  "description": "Turn YouTube playlists into video podcasts",
  "homepage": "https://github.com/cprecioso/tubecaster#readme",
  "bugs": {
    "url": "https://github.com/cprecioso/tubecaster/issues"
  },
  "license": "ISC",
  "author": "Carlos Precioso <kalprestito@gmail.com>",
  "files": [
    "lib",
    "public/**/*.styl",
    "views"
  ],
  "main": "lib/index.js",
  "bin": {
    "tubecaster": "lib/start.js"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://cprecioso@github.com/cprecioso/tubecaster.git"
  },
  "scripts": {
    "build": "yarn && yarn run prepublish",
    "clean": "rm -rf lib",
    "format": "prettier --write \"src/**/*\" \"types/**/*\"",
    "prepublish": "yarn run clean && tsc -p .",
    "start": "node lib/start.js",
    "start:nocache": "TUBECASTER_CACHE=no node lib/start.js",
    "watch": "yarn run clean && tsc --watch -p ."
  },
  "dependencies": {
    "apicache": "^1.2.3",
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "feedparser": "^2.2.9",
    "get-stream": "^4.0.0",
    "lodash": "^4.17.11",
    "most": "^1.7.3",
    "most-limiter": "^0.0.11",
    "most-node-streams": "^1.0.0",
    "node-cache": "^4.1.1",
    "parse-author": "^2.0.0",
    "pug": "^2.0.1",
    "pump": "^3.0.0",
    "rss": "^1.2.2",
    "stylus": "^0.54.5",
    "ytdl-core": "^0.25.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.9",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.16.0",
    "@types/feedparser": "^2.2.1",
    "@types/get-stream": "^3.0.1",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.9.4",
    "@types/pump": "^1.0.1",
    "@types/rss": "^0.0.28",
    "prettier": "^1.14.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  },
  "engines": {
    "node": ">=8"
  },
  "engineStrict": true
}
